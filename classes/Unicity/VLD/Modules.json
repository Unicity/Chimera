	},
	"eq_length": {
		"class": "\\Unicity\\VLD\\Module\\IsEqualToLength"
	},
	"ge": {
		"class": "\\Unicity\\VLD\\Module\\IsGreaterThanOrEqualTo"
	},
	"ge_length": {
		"class": "\\Unicity\\VLD\\Module\\IsGreaterThanOrEqualToLength"
	},
	"gt": {
		"class": "\\Unicity\\VLD\\Module\\IsGreaterThan"
	},
	"gt_length": {
		"class": "\\Unicity\\VLD\\Module\\IsGreaterThanLength"
	},
	"iban": {
		"class": "\\Unicity\\VLD\\Module\\IsIBAN"
	},
	"in": {
		"class": "\\Unicity\\VLD\\Module\\IsEnum"
	},
	"le": {
		"class": "\\Unicity\\VLD\\Module\\IsLesserThanOrEqualTo"
	},
	"le_length": {
		"class": "\\Unicity\\VLD\\Module\\IsLesserThanOrEqualToLength"
	},
	"lt": {
