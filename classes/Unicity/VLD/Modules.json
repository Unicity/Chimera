	},
	"regex": {
		"class": "\\Unicity\\VLD\\Module\\MatchesRegex"
	},
	"required": {
		"class": "\\Unicity\\VLD\\Module\\IsRequired"
	},
	"schema": {
		"class": "\\Unicity\\VLD\\Module\\MatchesSchema"
	},
	"undefined": {
