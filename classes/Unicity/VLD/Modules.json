	"bic": {
		"class": "\\Unicity\\VLD\\Module\\MatchesRegex",
		"policy": "/^([a-zA-Z]){4}([a-zA-Z]){2}([0-9a-zA-Z]){2}([0-9a-zA-Z]{3})?$/"
	},
	"eq": {
